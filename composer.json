  "require": {
    "contao/core-bundle":"^4.1"
  },
  "extra": {
    "branch-alias": {
      "master": "0.1.x-dev"
    },
    "contao-manager-plugin": "agentur1601com\\metaMandatory\\ContaoManager\\Plugin"
