{
  "name": "agentur1601com/meta-mandatory",
  "type": "contao-bundle",
  "description": "Change page and file meta fields to mandatory",
  "homepage": "https://1601.com",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Lukas Beck",
      "email"   : "lb@1601.com",
      "homepage": "https://github.com/LBeckX"
    },
    {
      "name": "Frank Ficzak",
      "homepage": "https://github.com/ff1601com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "contao/core-bundle": "4.*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "extra": {
    "contao-manager-plugin": "Agentur1601com\\MetaMandatory\\ContaoManager\\Plugin"
  },
  "autoload": {
    "psr-4": {
      "Agentur1601com\\MetaMandatory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Agentur1601com\\MetaMandatory\\Tests\\": "tests/"
    }
  }
